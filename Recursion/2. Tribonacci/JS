// https://leetcode.com/problems/n-th-tribonacci-number
// (TLE solutions - not the most optimised solutions)

/**
 * @param {number} n
 * @return {number}
 */
var tribonacci = function(n) {
    if (n <= 1) {
        return n;
    }
    if (n === 2) {
        return 1;
    }
    return tribonacci(n-1) + tribonacci(n-2) + tribonacci(n-3)
};